{
  "name": "formDagger",
  "description": "jump within form",
  "version": "1.4",
  "permissions": [
    "storage","tabs", "http://*/*", "https://*/*"
  ],
  "icons": { "16": "resource/iconOn.png", "48": "resource/iconOn.png", "128": "resource/iconOn.png" },
  "browser_action": {
    "default_title": "formDagger",
    "default_icon": "resource/iconOff.png"
  },
  "background": {
    "scripts": ["data/postpaidGen.js","data/prepaidGen.js","data/bundlesGen.js","background.js"]
  },
  "options_page": "options.html",
  "manifest_version": 2
}